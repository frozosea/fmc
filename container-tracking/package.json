  "scripts": {
    "build": "tsc -p .",
    "start": "ts-node ./src/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate src/db/migration/ -d src/db/data-source.ts",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/db/data-source.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/db/data-source.ts",
    "test:integration": "mocha --timeout 100000 --parallel -r ts-node/register tests/**/integrate_*_test.ts --recursive",
    "test:unit": "mocha --timeout 100000 --parallel -r ts-node/register tests/**/unit_*_test.ts ",
    "proto-gen": "cd src/server/proto && sh build.sh",
    "entrypoint": "npm run migration:up && npm run start"
  }
